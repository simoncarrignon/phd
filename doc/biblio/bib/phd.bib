	number = "1",
	pages = "5",
	title = "{Agent-Based Models and Simulations in Economics and Social Sciences: From Conceptual Exploration to Distinct Ways of Experimenting}",
	url = "http://jasss.soc.surrey.ac.uk/13/1/5.html",

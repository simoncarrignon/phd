@article{weber1998thedispositioneffectinsecuritiestradinganexperimentalanalysis,
	abstract = "The {\lq}disposition effect{\rq} is the tendency to sell assets that have gained value ({\lq}winners{\rq}) and keep assets that have lost value ({\lq}losers{\rq}). Disposition effects can be explained by the two features of prospect theory: the idea that people value gains and losses relative to a reference point (the initial purchase price of shares), and the tendency to seek risk when faced with possible losses, and avoid risk when a certain gain is possible. Our experiments were designed to see if subjects would exhibit disposition effects. Subjects bought and sold shares in six risky assets. Asset prices fluctuated in each period. Contrary to Bayesian optimization, subjects did tend to sell winners and keep losers. When the shares were automatically sold after each period, the disposition effect was greatly reduced.",
	author = "Weber, Martin and Camerer, Colin F.",
	doi = "10.1016/S0167-2681(97)00089-9",
	issn = "0167-2681",
	journal = "Journal of Economic Behavior \& Organization",
	keywords = "Disposition effect; Prospect theory; Trading volume",
	localfile = "~/projects/PhD/biblio/pdfs/weber1998thedispositioneffectinsecuritiestradinganexperimentalanalysis.pdf",
	number = "2",
	pages = "167--184",
	title = "{The disposition effect in securities trading: an experimental analysis}",
	url = "http://www.sciencedirect.com/science/article/pii/S0167268197000899",
	volume = "33",
	year = "1998"
}

@article{frydman2015usingneuraldatatotestatheoryofinvestorbehavioranapplicationtorealizationutility,
	abstract = "We conduct a study in which subjects trade stocks in an experimental market while we measure their brain activity using functional magnetic resonance imaging. All of the subjects trade in a suboptimal way. We use the neural data to test a ``realization utility'' explanation for their behavior. We find that activity in two areas of the brain that are important for economic decision-making exhibit activity consistent with the predictions of realization utility. These results provide support for the realization utility model. More generally, they demonstrate that neural data can be helpful in testing models of investor behavior.",
	author = "FRYDMAN, CARY and BARBERIS, NICHOLAS and Camerer, Colin F. and BOSSAERTS, PETER and RANGEL, ANTONIO",
	doi = "10.1111/jofi.12126",

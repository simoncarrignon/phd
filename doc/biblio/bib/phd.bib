	localfile = "~/projects/PhD/biblio/pdfs/frydman2015usingneuraldatatotestatheoryofinvestorbehavioranapplicationtorealizationutility.pdf",
	number = "2",
	pages = "907--946",
	title = "{Using Neural Data to Test a Theory of Investor Behavior: An Application to Realization Utility}",
	url = "http://dx.doi.org/10.1111/jofi.12126",
